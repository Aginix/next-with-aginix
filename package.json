    "eslint": "7.29.0",
    "eslint-config-next": "11.1.0",
    "faker": "^5.5.3",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "prisma": "^3.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
